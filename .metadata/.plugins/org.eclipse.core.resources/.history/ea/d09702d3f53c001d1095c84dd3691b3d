import java.util.Scanner;

public class ViDU {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s;
		System.out.println("Nhap vao chuoi: ");
		s = sc.nextLine();
		System.out.println("------");
		System.out.println(s.length());
		System.out.println(s.charAt(0));
	}
}
